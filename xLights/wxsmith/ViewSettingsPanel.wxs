<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="ViewSettingsPanel">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxGridBagSizer" variable="GridBagSizer1" member="no">
			<object class="sizeritem">
				<object class="wxStaticText" name="wxID_ANY" variable="StaticText1" member="no">
					<label>Effect Icon Size</label>
				</object>
				<col>0</col>
				<row>0</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxChoice" name="ID_CHOICE3" variable="ToolIconSizeChoice" member="yes">
					<content>
						<item>Small</item>
						<item>Medium</item>
						<item>Large</item>
						<item>Extra Large</item>
					</content>
					<selection>1</selection>
					<handler function="OnToolIconSizeChoiceSelect" entry="EVT_CHOICE" />
				</object>
				<col>1</col>
				<row>0</row>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="wxID_ANY" variable="StaticText4" member="no">
					<label>Model Handle Size</label>
				</object>
				<col>0</col>
				<row>1</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxChoice" name="ID_CHOICE4" variable="ModelHandleSizeChoice" member="yes">
					<content>
						<item>Normal</item>
						<item>Large</item>
						<item>Extra Large</item>
					</content>
					<selection>0</selection>
					<handler function="OnModelHandleSizeChoiceSelect" entry="EVT_CHOICE" />
				</object>
				<col>1</col>
				<row>1</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="wxID_ANY" variable="StaticText5" member="no">
					<label>Effect Assist Window</label>
				</object>
				<col>0</col>
				<row>2</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxChoice" name="ID_CHOICE5" variable="EffectAssistChoice" member="yes">
					<content>
						<item>Always On</item>
						<item>Always Off</item>
						<item>Auto Toggle</item>
					</content>
					<selection>2</selection>
					<handler function="OnEffectAssistChoiceSelect" entry="EVT_CHOICE" />
				</object>
				<col>1</col>
				<row>2</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX1" variable="PlayControlsCheckBox" member="yes">
					<label>Show Play Controls on Preview</label>
					<checked>1</checked>
					<handler function="OnPlayControlsCheckBoxClick" entry="EVT_CHECKBOX" />
				</object>
				<colspan>2</colspan>
				<col>0</col>
				<row>3</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX2" variable="HousePreviewCheckBox" member="yes">
					<label>Auto Show House Preview</label>
					<checked>1</checked>
					<handler function="OnHousePreviewCheckBoxClick" entry="EVT_CHECKBOX" />
				</object>
				<colspan>2</colspan>
				<col>0</col>
				<row>4</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX5" variable="CheckBox_DisableKeyAcceleration" member="yes">
					<label>Disable key acceleration when held down</label>
				</object>
				<col>0</col>
				<row>9</row>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX3" variable="CheckBox_BaseShowFolder" member="yes">
					<label>Enable Base Show Folder Settings</label>
					<handler function="OnCheckBox_BaseShowFolderClick" entry="EVT_CHECKBOX" />
				</object>
				<col>0</col>
				<row>5</row>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="wxID_ANY" variable="StaticText6" member="no">
					<label>Timeline Zooming</label>
				</object>
				<col>0</col>
				<row>6</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxChoice" name="ID_CHOICE_TIMELINEZOOMING" variable="Choice_TimelineZooming" member="yes">
					<content>
						<item>Play Marker Position</item>
						<item>Mouse Marker Position</item>
					</content>
					<selection>0</selection>
					<handler function="OnChoice_TimelineZoomingSelect" entry="EVT_CHOICE" />
				</object>
				<col>1</col>
				<row>6</row>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX4" variable="CheckBox_PresetPreview" member="yes">
					<label>Hide Preset Previews</label>
					<handler function="OnPresetPreviewCheckBoxClick" entry="EVT_CHECKBOX" />
				</object>
				<colspan>2</colspan>
				<col>0</col>
				<row>7</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX_ZoomMethod" variable="CheckBox_ZoomMethod" member="yes">
					<label>Zoom To Cursor</label>
					<checked>1</checked>
					<handler function="OnCheckBox_ZoomMethodClick" entry="EVT_CHECKBOX" />
				</object>
				<col>0</col>
				<row>8</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
