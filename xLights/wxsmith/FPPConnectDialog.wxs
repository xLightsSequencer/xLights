					<object class="sizeritem">
						<object class="wxButton" name="wxID_CANCEL" variable="cancelButton" member="no">
							<label>Cancel</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
