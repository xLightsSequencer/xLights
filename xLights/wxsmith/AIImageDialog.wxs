<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="AIImageDialog">
		<title>Generate Image</title>
		<centered>1</centered>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER|wxCLOSE_BOX|wxFULL_REPAINT_ON_RESIZE</style>
		<handler function="OnResize" entry="EVT_SIZE" />
		<object class="wxFlexGridSizer" variable="MainSizer" member="yes">
			<cols>1</cols>
			<growablecols>0</growablecols>
			<growablerows>1</growablerows>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="ParametersBox" member="yes">
					<label>Parameters</label>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="ParametersSizer" member="yes">
							<cols>2</cols>
							<growablecols>1</growablecols>
							<growablerows>0</growablerows>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
									<label>Prompt</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxTextCtrl" name="ID_PROMPT" variable="PromptBox" member="yes">
									<size>400,50</size>
									<focused>1</focused>
									<help>Enter a prompt</help>
									<style>wxTE_MULTILINE|wxVSCROLL</style>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>2</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="ResultsBox" member="yes">
					<label>Results</label>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="ResultBoxSizer" member="yes">
							<cols>3</cols>
							<growablecols>1</growablecols>
							<growablerows>0</growablerows>
							<object class="sizeritem">
								<object class="wxPanel" name="ID_PANEL1" variable="ImagePanel" member="yes">
									<minsize>500,420</minsize>
									<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
										<cols>2</cols>
										<rows>1</rows>
										<growablecols>0</growablecols>
										<growablerows>0</growablerows>
										<object class="sizeritem">
											<object class="wxPanel" name="ID_PANEL2" subclass="CropPanel" variable="cropPanel" member="yes">
												<minsize>400,400</minsize>
											</object>
											<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
												<cols>1</cols>
												<object class="sizeritem">
													<object class="wxStaticText" name="wxID_ANY" variable="SizeLabel1" member="no">
														<label>Size:</label>
													</object>
													<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxStaticText" name="ID_STATICTEXT4" variable="SizeLabel" member="yes">
														<label>0x0</label>
													</object>
													<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxButton" name="ID_RESIZE" variable="ResizeButton" member="yes">
														<label>Resize</label>
														<handler function="OnResizeButtonClick" entry="EVT_BUTTON" />
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxButton" name="ID_CROP" variable="CropButton" member="yes">
														<label>Crop</label>
														<handler function="OnCropButtonClick" entry="EVT_BUTTON" />
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxButton" name="ID_RESET" variable="ResetButton" member="yes">
														<label>Reset</label>
														<handler function="OnResetButtonClick" entry="EVT_BUTTON" />
													</object>
													<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
													<border>5</border>
													<option>1</option>
												</object>
											</object>
											<flag>wxALL|wxEXPAND</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxTextCtrl" name="ID_STATICTEXT2" variable="ErrorText" member="yes">
									<minsize>500,400</minsize>
									<style>wxTE_MULTILINE|wxTE_READONLY|wxTE_WORDWRAP|wxVSCROLL</style>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
					<cols>4</cols>
					<rows>1</rows>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxButton" name="ID_GENERATE" variable="GenerateButton" member="yes">
							<label>Generate</label>
							<default>1</default>
							<handler function="OnGenerateButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="spacer">
						<flag>wxALL|wxEXPAND</flag>
						<border>10</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="SaveButton" member="yes">
							<label>Save</label>
							<handler function="OnSaveButtonClicked" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_OK" variable="CloseButton" member="yes">
							<label>Close</label>
							<handler function="OnCloseButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>2</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
