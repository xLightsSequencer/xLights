														<hgap>10</hgap>
													</object>
													<flag>wxALL|wxEXPAND</flag>
													<border>5</border>
													<option>1</option>
												</object>
												<object class="sizeritem">
													<object class="wxFlexGridSizer" variable="PaintFuntionsSizer" member="no">
														<cols>1</cols>
														<object class="sizeritem">
															<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
																<cols>6</cols>
																<object class="sizeritem">
																	<object class="wxButton" name="ID_BUTTON_NewImage" variable="Button_NewImage" member="yes">
																		<label>New&#x0A;Image</label>
																		<handler function="OnButton_NewImageClick" entry="EVT_BUTTON" />
																	</object>
																	<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
																	<border>5</border>
																	<option>1</option>
																</object>
																<object class="sizeritem">
																	<object class="wxButton" name="ID_BUTTON_LoadImage" variable="Button_LoadImage" member="yes">
																		<label>Load&#x0A;Image</label>
																		<handler function="OnButton_LoadImageClick" entry="EVT_BUTTON" />
																	</object>
																	<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
																	<border>5</border>
																	<option>1</option>
																</object>
																<object class="sizeritem">
																	<object class="wxButton" name="ID_BUTTON_SaveImage" variable="Button_SaveImage" member="yes">
																		<label>Save&#x0A;Image</label>
																		<handler function="OnButton_SaveImageClick" entry="EVT_BUTTON" />
																	</object>
																	<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
																	<border>5</border>
																	<option>1</option>
																</object>
																<object class="sizeritem">
																	<object class="wxButton" name="ID_BUTTON_SaveAs" variable="Button_SaveAs" member="yes">
