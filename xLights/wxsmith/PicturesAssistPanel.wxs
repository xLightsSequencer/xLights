<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="PicturesAssistPanel">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxTAB_TRAVERSAL|wxWANTS_CHARS</style>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="yes">
			<cols>1</cols>
			<rows>1</rows>
			<object class="sizeritem">
				<object class="wxPanel" name="ID_PANEL1" variable="Panel_Sizer" member="yes">
					<style>wxTAB_TRAVERSAL|wxWANTS_CHARS</style>
					<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
						<cols>1</cols>
						<object class="sizeritem">
							<object class="wxScrolledWindow" name="ID_SCROLLED_EffectAssist" variable="ScrolledWindowEffectAssist" member="yes">
								<style>wxWANTS_CHARS|wxVSCROLL|wxHSCROLL</style>
								<object class="wxFlexGridSizer" variable="FlexGridSizer_Container" member="yes">
									<cols>2</cols>
									<growablecols>0</growablecols>
									<growablerows>0</growablerows>
									<object class="sizeritem">
										<object class="wxPanel" name="ID_PANEL_RightSide" variable="Panel_RightSide" member="yes">
											<object class="wxFlexGridSizer" variable="FlexGridSizer_RightSide" member="yes">
												<cols>1</cols>
												<object class="sizeritem">
													<object class="wxFlexGridSizer" variable="ColorPickerSizer" member="no">
														<cols>1</cols>
													</object>
													<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
